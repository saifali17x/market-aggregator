{
  "name": "market-aggregator-backend",
  "version": "1.0.0",
  "description": "Backend API for Market Aggregator",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "worker": "node workers/index.js",
    "worker:dev": "nodemon workers/index.js",
    "db:seed": "node scripts/seed-database.js",
    "setup:db": "node scripts/setup-database.js",
    "seed:db": "node scripts/seed-database.js",
    "migrate:db": "npx sequelize-cli db:migrate",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --detectOpenHandles",
    "migrate:ci": "npx sequelize-cli db:migrate || true",
    "seed:test": "node tests/helpers/seedTestDB.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "docker:build": "docker build -f Dockerfile.worker -t market-aggregator-worker .",
    "docker:up": "docker-compose up -d redis worker",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f worker",
    "sites:validate": "node ../scripts/validate-site-configs.js validate",
    "security:check": "node ../scripts/validate-site-configs.js security",
    "compliance:report": "node ../scripts/validate-site-configs.js report"
  },
  "dependencies": {
    "@bull-board/api": "^5.23.0",
    "@bull-board/express": "^5.23.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron-parser": "^4.9.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "playwright": "^1.40.1",
    "robots-parser": "^3.0.1",
    "sequelize": "^6.35.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "marketplace",
    "aggregator",
    "api",
    "express",
    "postgresql",
    "sequelize",
    "scraping"
  ],
  "author": "Saif Ali",
  "license": "MIT"
}
